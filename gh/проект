package javaio;
import java.io.File;
public class Main {
    public static void main(String[] args) {
      File d = new File (pathname: "Z:/test");
      d.mkdir(); // соз дирек
      for (int i = 0; i<20; i++) {
        File f = new File(d, "test_" + i + ".txt");
        f.createNewFile();
      }
      
      for (File f : d.listFiles ()){
        System.out.println(f.getName ());
      
      }
  }
}